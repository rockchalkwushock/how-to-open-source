{
  "name": "how-to-open-source",
  "version": "1.1.0",
  "license": "MIT",
  "description": "A repository for learning how to publish NPM packages as well as contribute to open source projects.",
  "author": "Cody Brunner <rockchalkwushock@iclould.com> (https://rockchalkwushock.github.io/rcws-development/)",
  "homepage": "https://github.com/rockchalkwushock/how-to-open-source#readme",
  "repository": "https://github.com/rockchalkwushock/how-to-open-source.git",
  "bugs": "https://github.com/rockchalkwushock/how-to-open-source/issues",
  "browser": "dist/how-to-open-source.js",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "dist",
    "es",
    "lib",
    "LICENSE"
  ],
  "keywords": [
    "commitizen",
    "open-source",
    "npm",
    "nps",
    "rollup",
    "semantic-release",
    "yarn"
  ],
  "scripts": {
    "build": "nps build.andTest",
    "commit": "nps commit",
    "prebuild": "nps validate.withTests",
    "precommit": "lint-staged && nps validate",
    "prettier": "prettier --single-quote --print-width 80 --trailing-comma all --no-semi --use-tabs",
    "start": "nps",
    "test": "nps test"
  },
  "publishConfig": {
    "access": "public"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.js": [
      "yarn prettier",
      "git add"
    ]
  },
  "devDependencies": {
    "babel-cli": "~6.24.1",
    "babel-core": "~6.25.0",
    "babel-plugin-external-helpers": "~6.22.0",
    "babel-plugin-syntax-flow": "~6.18.0",
    "babel-plugin-transform-flow-strip-types": "~6.22.0",
    "babel-preset-env": "~1.5.2",
    "codecov": "~2.2.0",
    "commitizen": "~2.9.6",
    "cz-conventional-changelog": "~2.0.0",
    "eslint": "~4.1.1",
    "eslint-config-equimper": "~2.1.2",
    "eslint-config-prettier": "~2.2.0",
    "eslint-plugin-flowtype": "~2.34.0",
    "flow-bin": "~0.49.1",
    "flow-typed": "~2.1.2",
    "husky": "~0.14.1",
    "jest": "~20.0.4",
    "jest-cli": "~20.0.4",
    "lint-staged": "~4.0.0",
    "nps": "~5.3.1",
    "nps-utils": "~1.2.0",
    "prettier": "~1.5.2",
    "rollup": "~0.43.0",
    "rollup-plugin-babel": "~2.7.1",
    "rollup-plugin-cleanup": "~1.0.0",
    "rollup-plugin-conditional": "~1.1.1",
    "rollup-plugin-filesize": "~1.4.2",
    "rollup-plugin-flow": "~1.1.1",
    "rollup-plugin-gzip": "~1.2.0",
    "rollup-plugin-multi-dest": "~1.0.2",
    "rollup-plugin-uglify": "~2.0.1",
    "semantic-release": "~6.3.2"
  }
}
