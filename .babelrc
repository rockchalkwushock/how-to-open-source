{
  "presets": ["es2015", "react", "stage-0"],
  "plugins": [
    "syntax-flow",
    "transform-flow-strip-types"
  ],
  "env": {
    "commonjs": {
      "plugins": [
        ["transform-es2015-modules-commonjs", { "loose": true }],
        "syntax-flow",
        "transform-flow-strip-types"
      ]
    },
    "es": {
      "plugins": [
        "syntax-flow",
        "transform-flow-strip-types"
      ]
    }
  }
}
